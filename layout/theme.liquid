<!doctype html>
<script>
  window.SLIDECART_DISABLE = true;
</script>
<html class="no-js" lang="{{ request.locale.iso_code }}" dir="{{ settings.text_direction }}">
  {% comment %} theme.liquid is the main file for liquid templates - starting point for all pages {% endcomment %}
  <head>{% render 'ymq_image-initializer' %}

<!-- HOTJAR CODE -->

    <!-- Hotjar Tracking Code for www.pupring.com -->
